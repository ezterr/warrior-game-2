{
  "compilerOptions": {
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "sourceMap": true,
    "target": "es6",
    "downlevelIteration": true,
    "lib": ["dom", "es6", "dom.iterable"],
    "outDir": "dist",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "strict": true
  }
}
